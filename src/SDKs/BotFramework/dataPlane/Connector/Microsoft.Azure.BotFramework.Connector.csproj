<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$([MSBuild]::GetPathOfFileAbove('AzSdk.reference.props'))" />
  <PropertyGroup>
    <PackageId>Microsoft.Azure.BotFramework.Connector</PackageId>
    <Description>Provides API functions for consuming the Microsoft BotFramework Connector API.</Description>
    <VersionPrefix>2.0.0</VersionPrefix>
    <DefineConstants>$(DefineConstants);CODESIGN</DefineConstants>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <AssemblyName>Microsoft.Azure.BotFramework.Connector</AssemblyName>
    <PackageTags>Microsoft BotFramework;Bots;BotBuilder</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>net452;netstandard1.4</TargetFrameworks>
  </PropertyGroup>
</Project>